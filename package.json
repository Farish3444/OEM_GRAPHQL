{
  "name": "oem_graphql",
  "description": "Bike Bandit - OEM Availability GraphQL",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "git clean -dfqX -- ./node_modules **/{dist,node_modules}/ ./packages/*/tsconfig*tsbuildinfo",
    "compile": "tsc --build tsconfig.build.json",
    "compile:clean": "tsc --build tsconfig.build.json --clean",
    "watch": "tsc --build tsconfig.json --watch",
    "start": "nodemon -e js,graphql,json --watch . start.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "apollo-server": "^3.1.2",
    "apollo-server-lambda": "^3.1.2",
    "chrome-aws-lambda": "^10.1.0",
    "eslint-plugin-graphql": "^4.0.0",
    "graphql": "^15.5.1",
    "puppeteer-core": "^10.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^8.2.1",
    "puppeteer": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
